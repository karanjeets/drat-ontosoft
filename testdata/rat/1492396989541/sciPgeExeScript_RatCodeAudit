#!sh
export PATH=$HOME/bin/:${PATH}
shopt -s expand_aliases
alias rat="java -jar /gpfs/flash/users/tg830544/drat-ontosoft/deploy/rat/lib/apache-rat-0.12.jar"
echo "Creating working dirs"
mkdir /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/input ; mkdir /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/output; mkdir /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/logs
echo "Staging input to /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/input"
FILELIST=`python -c "print ' '.join('/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/bootRes-internal.rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/rt.prec.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/mdcplot.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/mdcc.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/muc.clim.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/muc.fake.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/dcc.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/dcplot.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/bootRes-package.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/muc.spruce.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/rt.temp.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/man/rt.spruce.Rd,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/inst/CITATION,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/inst/Changelog,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/data/muc.clim.RData,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/data/muc.spruce.rda,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/data/rt.temp.RData,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/data/rt.prec.RData,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/data/rt.spruce.rda,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/data/muc.fake.RData,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/NAMESPACE,/gpfs/flash/users/tg830544/drat-ontosoft/softwares/github/c4p/cszang-bootRes/DESCRIPTION'.split(','))"`
for file in $FILELIST; do
rsync -av --backup --suffix=_`date +"%m%d%Y_%H%M"` $file /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/input
done
echo "Running Apache RAT on /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/input"
rat /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/input > /gpfs/flash/users/tg830544/drat-ontosoft/deploy/data/jobs/rat/1492396989541/output/rat_octet-stream_1492396989541.log

